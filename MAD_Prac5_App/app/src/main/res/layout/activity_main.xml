                    android:text="0"
                    android:textSize="16sp"
                    app:cornerRadius="10dp"
                    app:iconPadding="0dp" />

                <Button
                    android:id="@+id/button20"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="2dp"
